datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x", "debian-openssl-3.0.x"]
}

model Score {
  id        Int      @id @default(autoincrement())
  name      String
  score     Int
  difficulty Int
  createdAt DateTime @default(now())
}

