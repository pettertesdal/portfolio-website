datasource db {
  provider = "postgresql"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x", "linux-musl-openssl-3.0.x"]
}

model Score {
  id         Int    @id @default(autoincrement())
  name       String
  score      Int
  difficulty Int

  @@unique([name, difficulty])
}

